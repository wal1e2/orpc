{
  "name": "@orpc/svelte-kit-playground",
  "type": "module",
  "version": "1.13.4",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
